                        @Html.Partial("Navbar/Header/_FilterCars.cshtml")
                    </div>
                </div>
                <div class="header__indicators">
                    <div class="indicator indicator--trigger--click">
                        @Html.Partial("Navbar/Header/_UserScreen.cshtml")
