<body>
    <!-- site -->
    <div class="site">
        <!-- site__mobile-header -->
        @Html.Partial("Navbar/Header/_MobileHeader.cshtml")
        <!-- site__mobile-header / end -->
        <!-- site__header -->
        <header class="site__header">
            <div class="header">
                <div class="header__megamenu-area megamenu-area"></div>
